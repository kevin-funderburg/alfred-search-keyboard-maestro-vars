				<string>use scripting additionsuse kl : script "Kevin's Library"

on run argv
	set q to encodeXML(item 1 of argv)
	tell application "Keyboard Maestro Engine"				do script "&lt;dict&gt;
		&lt;key&gt;Action&lt;/key&gt;
		&lt;string&gt;DisplayWindow&lt;/string&gt;
		&lt;key&gt;MacroActionType&lt;/key&gt;
		&lt;string&gt;InsertText&lt;/string&gt;
		&lt;key&gt;StyledText&lt;/key&gt;
		&lt;data&gt;&lt;/data&gt;
		&lt;key&gt;Text&lt;/key&gt;
		&lt;string&gt;" &amp; q &amp; "&lt;/string&gt;
	&lt;/dict&gt;"
	end tell
end run

on encodeXML(str)	set str to kl's SearchandReplace(str, "&amp;", "&amp;amp;")	set str to kl's SearchandReplace(str, "\"", "\\\"")	return strend encodeXML</string>
